
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        Int  @id @default(autoincrement())
  username  String @unique
  email     String @unique
  password  String @unique
}

model Admin {
  id        Int  @id @default(autoincrement())
  username  String @unique
  email     String @unique
  password  String @unique

}